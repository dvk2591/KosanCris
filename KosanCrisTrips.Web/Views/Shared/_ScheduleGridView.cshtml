<link href="~/Content/JqueyDataTables/jquery.dataTables.min.css" rel="stylesheet" />
<script src="~/js/jquery.dataTables.min.js"></script>
<script src="~/Scripts/Custom/ScheduleGrid.js"></script>
<script src="http://code.gijgo.com/1.3.0/js/gijgo.js" type="text/javascript"></script>
<link href="http://code.gijgo.com/1.3.0/css/gijgo.css" rel="stylesheet" type="text/css" />

<script src="https://cdn.datatables.net/plug-ins/9dcbecd42ad/integration/jqueryui/dataTables.jqueryui.js">
</script>
<link rel="stylesheet" href="https://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css">
<link rel="stylesheet" href="https://cdn.datatables.net/plug-ins/9dcbecd42ad/integration/jqueryui/dataTables.jqueryui.css">

@{ 
    List<string> names = new List<string>()
    { "Vamsi","Sainath", "Ravi", "Kumar", "Prasad", "Krishna","Satya","Seshu","Kiran","Raju","Venkat"};
}

<table id="scheduleGrid" class="table table-condensed table-striped table-hover">
    <thead>
        <tr>
            <th>
                #
            </th>
            <th>
                Employee
            </th>
            <th>
                Start Date
            </th>
            <th>
                End Date
            </th>
            <th>
            </th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <th>
                #
            </th>
            <th>
                Employee
            </th>
            <th>
                Start Date
            </th>
            <th>
                End Date
            </th>
            <th>
            </th>
        </tr>
    </tfoot>
    <tbody>
        @for (int i=1; i<=10; i++)
        {
            <tr>
                <td>
                    @i
                </td>
                <td>
                    @names[i]
                </td>
                <td>
                    @DateTime.Now.ToShortDateString()
                </td>
                <td>
                    @DateTime.Now.AddDays(10).ToShortDateString()
                </td>
            </tr>
        }
    </tbody>
</table>



